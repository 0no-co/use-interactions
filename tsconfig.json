{
  "compilerOptions": {
    "types": [
      "react",
      "cypress",
      "cypress-real-events",
      "cypress-promise/register"
    ],
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "esnext",
    "strict": true,
    "noImplicitAny": false,
    "noUnusedParameters": true
  }
}
