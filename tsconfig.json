{
  "include": ["./src"],
  "compilerOptions": {
    "types": ["react", "cypress", "cypress-real-events"],
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "target": "esnext",
    "strict": true,
    "noImplicitAny": false,
    "noUnusedParameters": true
  }
}
